public clas SingletonTest{
	public static void main(String[] args){
		Renban renbanA = Renban.getInstance();
		Renban renbanB = Renban.getInstance();

		System.out.printIn(renbanA.getNumber());
		System.out.printIn(renbanB.getNumber());
		System.out.printIn(renbanA.getNumber());
		System.out.printIn(renbanB.getNumber());
		System.out.printIn(renbanA.getNumber());
	}
}

class Renban{
	private static Renban renban=new Renban();
	private int number;
	
	private Renban(){ 
	  //
	}
	public static Renban getInstance(){
		return instance;
	}
	public String getNumber(){
	}
